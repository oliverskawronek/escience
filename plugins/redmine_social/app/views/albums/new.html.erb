<div class ='alt'>
    <%#= link_to l(:back_to_photo_manager), user_photo_manager_index_path(current_user) %>
</div>
  <div id='yui'>
    <div id ="single_upload", class =".yui-b">
      <h3><%= l(:new_album) %></h3>
      <%= form_for(@album, :url => user_albums_path(User.current.id), :html => {:class => "MainForm"}) do |f| %>
