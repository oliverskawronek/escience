<% if @update_messages %>
    $('#list_of_user_messages').html('<%= escape_javascript(render :partial => 'list_of_messages') %>');
<% end %>
<% message = flash[:error] || flash[:notice] || flash[:warning]
   if message
     type = flash.keys[0].to_s %>
     $.notification({ message:"<%= message%>", type:"<%= type %>" });
<% end %>